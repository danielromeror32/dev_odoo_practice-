<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="maestro_cedula_secuence" model="ir.sequence">
        <field name="name">Secuencia numero de cedula maestros</field>
        <field name="implementation">no_gap</field>
        <field name="code">maestro.cedula.secuence</field>
        <field name="prefix">C-</field>
        <!-- <field name="suffix"></field> -->
        <field name="padding">7</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="nomuero_alumno_secuence" model="ir.sequence">
        <field name="name">Secuencia numero de alumno</field>
        <field name="implementation">no_gap</field>
        <field name="code">alumno.secuence</field>
        <field name="prefix">A-</field>
        <!-- <field name="suffix"></field> -->
        <field name="padding">4</field>
        <field name="number_increment">1</field>
    </record>

</odoo>
